<div class="login-info">
    <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="fa fa-cog" aria-hidden="true"></i><span class="caret"></span></a>
        <ul class="dropdown-menu">
            <li><a href="/users/sign-out"><i class="fa fa-sign-out" aria-hidden="true"></i>Sign Out</a>
            </li>
            <form class="create-update-form" id="deletion" action="events/deleteaccount/" method="POST"><button id="delete" class="btn btn-default" type="submit"><i class="fa fa-user-times" aria-hidden="true"></i>Delete Account</button>
            </form>
        </ul>
    </li>

</div>

<div class="formWrapper">
    <form class="create-update-form" id="signin" method="POST" action=javascript:action>
        <div class="col-2">
            <label>
        Start Date
        <input type="date" id="em" tabindex="2" placeholder="YYYY-MM-DD" name="startDate" value="2016-01-01" required>
      </label>
        </div>
        <div class="col-2">
            <label>
        End Date
        <input type="date" tabindex="3" id="pass" placeholder="YYYY-MM-DD" name="endDate" value="2016-12-31" required>
      </label>
        </div>
        <button id='button' form="signin" value="Submit" type="submit" class="btn btn-default">GRAPH</button>

        <div class="dropD" id="introB">
            <select name="GroupId" id="GroupId" onChange="this.form.action=this.options[this.selectedIndex].value;">
        <option value="" disabled selected>Select an option</option>
        <option value="/graph/newRevenueByMonth">New Revenue by Month</option>
        <option value="/graph/allRevenueByMonth">All Revenue by Month</option>
        <option value="/graph/newRevenueSalesperson">New Revenue by Salesperson</option>
        <option value="/graph/allRevenueSalesperson">All Revenue by Salesperson</option>
        <option value="/graph/newRevenueProductLine">New Revenue by Product Line</option>
        <option value="/graph/allRevenueProductLine">All Revenue by Product Line</option>
        <option value="/graph/newRevenueDealType">New Revenue By Deal Type</option>
        <option value="/graph/allRevenueDealType">ALL Revenue By Deal Type</option>
        <option value="/graph/newRevenueVertical">New Revenue by Vertical</option>
        <option value="/graph/allRevenueVertical">All Revenue by Vertical</option>
        <option value="/graph/newRevenueProductLineCount">New Revenue Product Line (Count)</option>
        <option value="/graph/allRevenueProductLineCount">All Revenue Product Line (Count)</option>

     </select>
        </div>
    </form>
</div>
</div>
